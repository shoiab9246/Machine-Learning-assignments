    "import numpy as np\n",
    "\n",
    "#calculate the average of a list of numbers \n",
    "def mean(numbers):\n",
    "    return float(sum(numbers)) / max(len(numbers), 1)\n",
    "\n",
    "np.int(mean([42.2, 42.7, 41.1])) #report the integer or characteristic part of the average"
